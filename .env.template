# Copy to .env for local configuration

# AWS Bedrock configuration
AWS_REGION=us-east-1
# Vision-capable model to use in Bedrock
BEDROCK_MODEL_ID=meta.llama3-2-11b-instruct-v1:0
# AWS Credentials
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Optional upload size cap (bytes). Default ~6MB if unset.
# Example: 6291456
MAX_UPLOAD_BYTES=6291456
